<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Border-->
    <SolidColorBrush x:Key="Background0" Color="#F3F3F3"/>
    <SolidColorBrush x:Key="Background1" Color="#FBFBFB"/>
    <SolidColorBrush x:Key="Border1" Color="#DCDCDC"/>
    <SolidColorBrush x:Key="Background2" Color="#FEFEFE"/>
    <SolidColorBrush x:Key="Border2" Color="#E6E6E6"/>
    
    <!--Theme-->
    <SolidColorBrush x:Key="Theme1" Color="#16898f"/>
    <SolidColorBrush x:Key="Theme2" Color="#036163"/>
    
    <!--Text-->
    <SolidColorBrush x:Key="White" Color="{DynamicResource Background2}"/>
    <SolidColorBrush x:Key="Title" Color="#313131"/>
    <SolidColorBrush x:Key="Paragraph" Color="#6A6A6A"/>
</ResourceDictionary>